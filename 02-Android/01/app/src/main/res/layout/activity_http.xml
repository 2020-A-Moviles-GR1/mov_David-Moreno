<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HttpActivity">

    <Button
        android:id="@+id/btn_obtener"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="163dp"
        android:text="OPTENER"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_crear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="CREAR"
        app:layout_constraintEnd_toEndOf="@+id/btn_obtener"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btn_obtener"
        app:layout_constraintTop_toBottomOf="@+id/btn_obtener" />

    <Button
        android:id="@+id/btn_usuario_conver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:text="USUARIO"
        app:layout_constraintEnd_toEndOf="@+id/btn_crear"
        app:layout_constraintStart_toStartOf="@+id/btn_crear"
        app:layout_constraintTop_toBottomOf="@+id/btn_crear" />

    <Button
        android:id="@+id/btn_pokemon_conv"
        android:layout_width="160dp"
        android:layout_height="42dp"
        android:layout_marginTop="82dp"
        android:text="POKEMON"
        app:layout_constraintEnd_toEndOf="@+id/btn_usuario_conver"
        app:layout_constraintStart_toStartOf="@+id/btn_usuario_conver"
        app:layout_constraintTop_toBottomOf="@+id/btn_usuario_conver" />
</androidx.constraintlayout.widget.ConstraintLayout>